{
  "name": "tunli",
  "description": "Node.js application for creating HTTP tunnels to make local software projects accessible over the internet.",
  "version": "0.0.25",
  "main": "bin/tunli",
  "bin": {
    "tunli": "bin/tunli"
  },
  "scripts": {
    "dev": "nodemon client.js"
  },
  "keywords": [
    "HTTP tunnel",
    "reverse proxy",
    "remote access",
    "local development",
    "Node.js"
  ],
  "author": "Pascal Pfeifer <pascal@tunli.app>",
  "imports": {
    "#lib/*": "./src/lib/*.js",
    "#commands/*": "./src/commands/*.js",
    "#src/*": "./src/*.js"
  },
  "type": "module",
  "dependencies": {
    "axios": "^1.7.2",
    "blessed": "^0.1.81",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tunlijs/tunli-client"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://tunli.app",
  "bugs": {
    "email": "dev@tunli.app",
    "url": "https://github.com/tunlijs/tunli-client/issues"
  },
  "license": "GPL-3.0",
  "readme": "README.md",
  "engines": {
    "node": ">=18"
  }
}
